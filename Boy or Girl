#include <bits/stdc++.h>

using namespace std;

int main()
{
    string N;

    cin>>N;

    set<char>s;

    for(char c : N)
        {
            s.insert(c);
        }
    if(s.size() % 2 == 0)
        {
            cout<< "CHAT WITH HER!";
        }

    else
        {
            cout<< "IGNORE HIM!";
        }
    return 0;
}
